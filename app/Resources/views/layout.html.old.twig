<!DOCTYPE html>
<html>
    <head>
        <title>{% block headtitle %}Certificats MROOT{% endblock %}</title>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
          {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('bootstrap/css/bootstrap-responsive.css') }}" type="text/css" />
        <link rel="stylesheet" href="{{ asset('bootstrap/css/bootstrap.css') }}" type="text/css" />
        <link rel="stylesheet" href="{{ asset('css/jquery-ui-1.9.2.custom.css') }}" type="text/css" media="all"/>
        <link rel="stylesheet" href="{{ asset('bundles/whiteoctoberpagerfanta/css/pagerfantaDefault.css') }}" type="text/css" media="all" />
        <link rel="stylesheet" href="{{ asset('bundles/applicationcertificats/css/certificatscenter.css') }}" type="text/css" media="all"/>
         {% endblock %}
    </head>
    <body>
        <div class="container">
            <div class="page-header">
                  {{ block('NAVIGATION_BAR' }}
                   {% use "::navbar.html.twig" %}
                </div>
                <div id="content">

 {#% block navbartitle %} MONBLOG{% endblock %#}
                 <center><h1>   {% block title %}CERTIFICATS{% endblock %}</h1></center>
                {% block content %}
                {% endblock %}
                </div>
                <div id="footer">
                    <p align="center">{{ 'copyright MROOT'|trans }} Â© 2012</p>
                </div>
                <!-- Le javascript ================================================== -->
                <!-- Placed at the end of the document so the pages load faster -->

       {% block javascripts %}
                <script src="{{ asset('jquery/jquery-1.8.3.js') }}" type="text/javascript"></script>
                <script type="text/javascript" src="{{ asset('jquery/jquery-ui-1.9.2.custom.min.js') }}"></script>
                <script type="text/javascript" src="{{ asset('jquery/jquery.ui.datepicker.js') }}"></script>
                <script type="text/javascript" src="{{ asset('bootstrap/js/bootstrap.js') }}"></script>
     {% endblock %}
     {#{ tinymce_init() }#}
            </div>
        </body>

    </html>