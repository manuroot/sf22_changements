<!DOCTYPE html>
    <html>
    <head>
        <title>{% block title %}Sticky Notes{% endblock %}</title>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
          {% block stylesheets %}
        
       
      <link rel="stylesheet" href="{{ asset('bootstrap/css/bootstrap.css') }}" type="text/css" />
      <link rel="stylesheet" href="{{ asset('bootstrap/css/bootstrap-responsive.css') }}" type="text/css" />
      <link rel="stylesheet" href="{{ asset('css/jquery-ui-1.9.2.custom.css') }}" type="text/css" media="all"/>
        <style type="text/css">
    /* */
   .page,.current,.next,.last{
    display: inline-block;
    border: 1px solid blue;
    color: blue;
    margin-right: 0.2em;
    padding: 0.25em 0.35em;
   }
    .current {
    
      background-color: #d3d3d3;
   /* color: red;*/
  
}
</style>
{% endblock %}
    </head>
      <body>
          
        
            <div class="page-header">
                   {% include "::navbar.html.twig" %}
               </div>
            <div id="content">
                       
  
              {# <h1>{{ block('title') }}</h1>#}
                {% block content %}
                {% endblock %}
            </div>
            <div id="footer">
                <p align="center">{{ 'copyright MROOT'|trans }} Â© 2012</p>
            </div>
       
    {% block javascripts %}
    <script src="{{ asset('jquery/jquery-1.8.3.js') }}" type="text/javascript"></script>
    <script type="text/javascript" src="{{ asset('jquery/jquery-ui-1.9.2.custom.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bootstrap/js/bootstrap.js') }}"></script>
   {% endblock %}
    </body>

</html>