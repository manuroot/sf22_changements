            var colors=["#94a2be","#993355","#aaaaaa","#f2a640","#b373b3","#668cd9"];
        return jQuery.inArray(value, colors);
    }
    function getcolorById(id) {
            var colors=["#94a2be","#993355","#aaaaaa","#f2a640","#b373b3","#668cd9"];
        if (id <= 5 )
